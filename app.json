{
  "name": "Durgesh ChatBot",
  "description": "Durgesh ChatBot is an AI-powered Telegram bot with smart responses, automation, and seamless integrations using Python, MongoDB, and Pyrogram.",
  "logo": "https://graph.org/file/826485f2d7db6f09db8ed.jpg",
  "keywords": [
    "Chatbot",
    "MongoDB",
    "Python",
    "Pyrogram"
  ],
  "buildpacks": [
    {
      "url": "heroku/python"
    },
    {
      "url": "https://github.com/jonathanong/heroku-buildpack-ffmpeg-latest.git"
    }
  ],
  "repository": "https://github.com/IamElite/ACB/tree/m1",
  "formation": {
    "worker": {
      "quantity": 1,
      "size": "Standard-2X"
    }
  },
  "env": {
    "API_ID": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": "24168862"
    },
    "API_HASH": {
      "description": "Get this value from my.telegram.org/apps.",
      "required": true,
      "value": "916a9424dd1e58ab7955001ccc0172b3"
    },
    "BOT_TOKEN": {
      "description": "A bot token from @BotFather.",
      "required": true,
      "value": ""
    },
    "MONGO_URL": {
      "description": "MongoDB connection URL from https://cloud.mongodb.com.",
      "required": true,
      "value": "mongodb+srv://Hello:Hello@cluster0.pi9ks.mongodb.net/?retryWrites=true&w=majority"
    },
    "AUTH_CHANNEL": {
      "description": "The Telegram channel ID for authentication.",
      "required": true,
      "value": "-1002372064638"
    },
    "FSUB": {
      "description": "Enable forced subscription (true/false).",
      "required": true,
      "value": "false"
    },
    "OWNER_ID": {
      "description": "User ID of the bot owner.",
      "required": true,
      "value": "7074383232"
    },
    "UPSTREAM_BRANCH": {
      "description": "M1",
      "required": true,
      "value": "m1"
    },
    "UPSTREAM_REPO": {
      "description": "repo",
      "required": true,
      "value": "https://github.com/IamElite/ACB"
    }
  }
}
